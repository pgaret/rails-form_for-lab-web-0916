<h1> Create School Class Form </h1>
<%= form_for @school_class do |f|%>
  <%= label_tag :title %>
  <%= text_field_tag :'school_class[title]', id: 'school_class_title' %>
  <%= label_tag :room_number %>
  <%= text_field_tag :'school_class[room_number]', id: 'school_class_room_number' %>

  <%= f.submit "Create School class" %>
<% end %>
